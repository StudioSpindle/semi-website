{
  "name": "semi-website",
  "version": "1.0.0",
  "description": "## Bundler is used",
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-sort-class-members": "^1.4.0",
    "esm": "^3.2.18",
    "file-loader": "^3.0.1",
    "html-minifier": "^3.5.20",
    "jsdom": "^14.0.0",
    "lodash.merge": "^4.6.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^6.0.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prettier": "1.16.4",
    "pubsub-js": "^1.7.0",
    "sass-loader": "^7.1.0",
    "scriptjs": "2.5.9",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "scripts": {
    "test": "mocha -r esm \"./_js-src/**/*.spec.js\"",
    "test:html": "htmlproofer ./_site --internal-domains \"www.semi.technology\" --allow-hash-href --url-ignore \"/codepen.io/\" --empty-alt-ignore --assume-extension --check-html",
    "jekyll:clean": "bundle exec jekyll clean",
    "jekyll:build": "bundle exec jekyll build --config _config.yml, _config_localhost.yml",
    "prelocal": "npm run build",
    "local": "bundle exec jekyll serve --livereload --config _config.yml, _config_localhost.yml",
    "htmlmin": "html-minifier --input-dir ./_site --output-dir ./_site --collapse-whitespace --file-ext html",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development --display-error-details",
    "build:watch": "webpack --mode=development --display-error-details --watch",
    "prettier": "prettier --check ./_js-src/**/* !./_js-src/lib/**/*",
    "eslint": "eslint ./_js-src/** --ignore-path ./_js-src/lib/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SeMI-network/semi-website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SeMI-network/semi-website/issues"
  },
  "homepage": "https://github.com/SeMI-network/semi-website#readme",
  "dependencies": {
    "jquery": "^3.3.1"
  }
}
